{
  "manifest_version": 2,

  "name": "My Extension",
  "description": "This extension is a starting point to create a real Chrome extension",
  "version": "0.0.1",
  "icons": {
    "16": "logo-rocket.png",
    "48": "logo-rocket.png",
    "128": "logo-rocket.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["bundle.js"]
  }]
}
